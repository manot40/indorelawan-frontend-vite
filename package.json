    "react-router-dom": "5.2.0",
    "react-scripts": "3.4.3",
    "react-share": "4.2.0",
    "react-slick": "0.28.1",
    "reactour": "1.18.0",
    "remarkable": "2.0.1",
    "slick-carousel": "1.8.1",
    "styled-components": "4.4.1",
    "swr": "0.2.3",
    "yup": "0.29.1"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "1.3.2",
    "eslint": "^8.20.0",
    "eslint-config-react-app": "^7.0.1",
    "prettier": "1.19.1",
    "sass": "1.49.9",
    "vite": "2.9.9",
    "vite-plugin-eslint": "^1.7.0",
    "vite-plugin-svgr": "2.1.0"
  },
  "prettier": {
    "endOfLine": "lf",
    "arrowParens": "always",
    "bracketSpacing": true,
    "printWidth": 170,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
