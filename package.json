    "react-scripts": "^3.4.3",
    "react-share": "^4.2.0",
    "react-slick": "^0.28.1",
    "reactour": "^1.18.0",
    "remarkable": "^2.0.1",
    "sass": "^1.49.9",
    "slick-carousel": "^1.8.1",
    "styled-components": "^4.4.1",
    "swr": "^0.2.3",
    "yup": "^0.29.1"
  },
  "scripts": {
    "check-format": "prettier --check \"src/**/*.{js,jsx}\"",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "start": "vite",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/*.stories.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 20,
        "branches": 20,
        "functions": 20,
        "lines": 20
      }
    }
  },
  "prettier": {
    "endOfLine": "lf",
    "arrowParens": "always",
    "bracketSpacing": true,
    "printWidth": 170,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^3.3.0",
    "@vitejs/plugin-react": "^1.3.2",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.13.1",
    "vite": "^2.9.9",
    "vite-plugin-svgr": "^2.1.0"
  }
}
