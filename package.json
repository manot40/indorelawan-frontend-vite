    "antd": "4.16.13",
    "axios": "0.21.1",
    "date-fns": "2.14.0",
    "dompurify": "2.2.7",
    "draft-js": "0.11.7",
    "formik": "^2.2.9",
    "html-react-parser": "1.2.4",
    "immutable": "4.0.0-rc.12",
    "jwt-decode": "2.2.0",
    "leaflet": "1.7.1",
    "lodash": "4.17.19",
    "markdown-draft-js": "2.2.0",
    "moment-timezone": "0.5.31",
    "query-string": "6.13.1",
    "react": "^17.0.2",
    "react-cookie": "4.0.3",
    "react-custom-scrollbars": "4.2.1",
    "react-dom": "^17.0.2",
    "react-draft-wysiwyg": "1.14.5",
