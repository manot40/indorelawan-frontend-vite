    "formik": "^2.1.4",
    "html-react-parser": "^1.2.4",
    "immutable": "^4.0.0-rc.12",
    "jwt-decode": "^2.2.0",
    "leaflet": "^1.7.1",
    "lodash": "^4.17.19",
    "markdown-draft-js": "^2.2.0",
    "moment-timezone": "^0.5.31",
    "query-string": "^6.13.1",
    "react": "^16.13.1",
    "react-cookie": "^4.0.3",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.13.1",
    "react-draft-wysiwyg": "^1.14.5",
    "react-ga": "^3.1.2",
    "react-google-recaptcha": "^2.1.0",
    "react-helmet": "^6.1.0",
