{
  "name": "frontend",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "check-format": "prettier --check \"src/**/*.{js,jsx}\"",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "start": "vite",
    "build": "vite build",
    "serve": "vite serve --host --port 3000"
  },
  "dependencies": {
    "@ant-design/icons": "4.1.0",
    "@sentry/react": "5.24.2",
    "@sentry/tracing": "5.24.2",
